{
    "name": "BAWES/whitebook",
    "description": "The White Book Online Event Planning Platform",
    "version": "2.1.0",
    "keywords": ["plan", "event"],
    "homepage": "http://www.thewhitebook.com.kw/",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "yiisoft/yii2-httpclient": "dev-master",
        "yiisoft/yii2-redis": "~2.0.0",
        "openbuildings/swiftmailer-css-inliner": "*",
        "costa-rico/yii2-images": "dev-master",
        "dektrium/yii2-user": "*",
        "arturoliveira/yii2-excelview": "*",
        "kartik-v/yii2-krajee-base": "*",
        "kartik-v/yii2-mpdf": "dev-master",
        "miloschuman/yii2-highcharts-widget": "*",
        "2amigos/yii2-file-upload-widget": "~1.0",
        "yiisoft/yii2-imagine": "*",
        "2amigos/yii2-ckeditor-widget": "*",
        "understeam/yii2-slack": "*",
        "2amigos/yii2-resource-manager-component": "*",
        "codemix/yii2-localeurls": "*",
        "kartik-v/yii2-widget-fileinput": "@dev"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*",
        "yiisoft/yii2-jui": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
